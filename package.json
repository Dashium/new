{
  "name": "gestion_nodejs_projects",
  "version": "1.0.0",
  "description": "gestionnaire de projets",
  "main": "mainjs",
  "scripts": {
    "start": "npm-run-all start:*",
    "start:infos": "node ./modules/infos.js",
    "start:build": "next build ./modules/dashboard/",
    "start2:export": "next export ./modules/dashboard/",
    "start:dashium": "npm-run-all --parallel dashium:*",
    "dashium:api": "node ./modules/api/main.js",
    "dashium:terminal": "node ./modules/terminal/main.js",
    "dashium:monitor": "node ./modules/monitor/main.js",
    "dashium:dashboard": "next start ./modules/dashboard/",
    "dashium2:dashboard": "node ./modules/dashboard/dashboard.js",
    "setup": "npm-run-all setup:*",
    "setup:infos": "node ./modules/infos.js",
    "setup:setup": "node ./setup/main.js",
    "uninstall": "node ./setup/uninstaller.js",
    
    "reset": "npm-run-all --continue-on-error reset:*",
    "reset:u": "npm run uninstall",
    "reset:s": "npm run setup",

    "nogui": "npm-run-all --parallel nogui:*",
    "nogui:api": "node ./modules/api/main.js",
    "nogui:terminal": "node ./modules/terminal/main.js",
    "nogui:monitor": "node ./modules/monitor/main.js",

    "dev": "next dev ./modules/dashboard",
    "build": "next build ./modules/dashboard",
    "startr": "next start ./modules/dashboard",
    "lint": "next lint ./modules/dashboard",
    "export": "next export ./modules/dashboard"
  },
  "keywords": [
    "gestionnaire",
    "projets",
    "nodejs",
    "github",
    "tai",
    "studio"
  ],
  "author": "Tai Tetsuyuki",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.7.11",
    "axios": "^1.3.5",
    "eslint-config-next": "^13.3.0",
    "express": "^4.18.2",
    "next": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "react-icons": "^4.8.0",
    "simple-git": "^3.17.0",
    "socket.io": "^4.6.1",
    "sqlite3": "^5.1.6",
    "xterm": "^5.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "css-loader": "^6.7.3",
    "eslint": "^8.33.0"
  }
}
